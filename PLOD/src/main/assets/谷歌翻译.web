				var e=new google.translate.TranslateElement(
					{ pageLanguage: 'auto' }
					//{pageLanguage:'auto',layout:google.translate.TranslateElement.InlineLayout.SIMPLE,multilanguagePage:true}
					, 'body');
				console.log('ret', e);
				window.gTransTime=setTimeout(window.gTrans, 200);
			}
		} else {
			window.gTransTime=setTimeout(window.gTrans, 200);
		}
